{
  "extends": ["eslint:recommended", "plugin:react/recommended", "plugin:prettier/recommended"],
  "parser": "babel-eslint",
  "rules": {
    "no-console": "off",
    "prettier/prettier": ["error", { "singleQuote": true }]
  },
  "plugins": ["prettier", "react"],
  "env": {
    "node": true,
    "browser": true,
    "jest": true
  }
}